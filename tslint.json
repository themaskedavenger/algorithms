{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-eslint-rules"
  ],
  "jsRules": {
    "indent": "off",
    "@typescript-eslint/indent": ["error", 2]
  },
  "rules": {
    "brace-style": false,
    "import-name": false,
    "indent": "off",
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "one-line": ["check-open-brace"],
    "prefer-array-literal": false,
    "variable-name": false,
    "@typescript-eslint/indent": ["error", 2]
  },
  "rulesDirectory": []
}
