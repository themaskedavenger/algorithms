{
  "version": "0.1.0",
  "description": "",
  "main": "",
  "scripts": {
    "build": "yarn run build:lint && yarn run build:transpile",
    "build:lint": "tslint src/**/*",
    "build:transpile": "yarn run build:deletebuild && tsc",
    "dist": "yarn run dist:del && yarn run build:transpile && yarn run dist:rollup && yarn run dist:terser && yarn run dist:obfuscate && mkdir dist && cp obfuscate/start.js dist",
    "dist:del": "del 'build' && del 'rollup' && del 'terser' && del obfuscate && del 'dist'",
    "dist:obfuscate": "javascript-obfuscator terser/start.js --output obfuscate/start.js --target node",
    "dist:rollup": "rollup --config",
    "dist:terser": "mkdir terser && terser rollup/start.js -o terser/start.js -m -c --comments '/$^/'",
    "build:deletebuild": "del 'build'",
    "sandbox": "ts-node sandbox.ts",
    "start:dev": "ts-node src/start.ts",
    "start:devbuild": "yarn run build:transpile && ts-node src/start.ts",
    "start:dist": "NODE_ENV='production' node dist/start.js",
    "start:prod": "NODE_ENV='production' node build/start.js",
    "test": "ava-ts --verbose",
    "test:file": "ava-ts --verbose src/test/test.ts",
    "watch:dev": "sane 'yarn run start:dev' --glob 'src/**'",
    "watch:devbuild": "sane 'yarn run start:devbuild' --glob 'src/**'"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@enmesh/testutils": "^0.1.23",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "ava": "^2.3.0",
    "ava-ts": "^0.25.2",
    "babel-preset-next": "^1.2.0",
    "copyfiles": "^2.1.1",
    "del-cli": "^3.0.0",
    "javascript-obfuscator": "^0.18.1",
    "renamer": "^1.1.2",
    "rollup": "^1.23.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "sane": "^4.1.0",
    "sinon": "^7.4.1",
    "terser": "^4.3.8",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3",
    "watch": "^1.0.2"
  },
  "ava": {
    "files": [
      "src/test/**/*"
    ],
    "require": [
      "@babel/preset-env",
      "@babel/polyfill",
      "@babel/register",
      "ts-node/register"
    ],
    "babel": "inherit",
    "extensions": [
      "js",
      "ts"
    ]
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "dotenv": "^8.1.0",
    "fs-extra": "^8.1.0",
    "merge-deep": "^3.0.2",
    "node-ini": "^1.0.0"
  }
}
