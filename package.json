{
  "version": "0.1.0",
  "description": "",
  "main": "",
  "scripts": {
    "build": "yarn run lint && yarn run test && yarn run transpile",
    "copyts": "copyfiles -u 1 src/**/* ts",
    "createts": "yarn run deletets && yarn run copyts && yarn run renamets",
    "deletebuild": "del 'build'",
    "deletets": "del 'ts'",
    "lint": "tslint src/**/*",
    "renamets": "renamer --force --find '.mts' --replace '.ts' './ts/**'",
    "sandbox": "ts-node sandbox.ts",
    "startdev": "yarn run transpile && ts-node src/startdev.ts",
    "start": "NODE_ENV='production' node build/start.js",
    "test": "ava-ts --verbose",
    "testbuild": "ava --verbose build/test/**/*",
    "testfile": "ava-ts --verbose src/test/test.ts",
    "transpile": "yarn run createts && yarn run deletebuild && tsc && yarn run deletets",
    "watch": "npx nodemon",
    "watcher": "yarn run watcherCode",
    "watcherJSX": "yarn run build && yarn run testfile",
    "watcherCode": "yarn run testfile"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@enmesh/testutils": "^0.1.23",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "ava": "^2.3.0",
    "ava-ts": "^0.25.2",
    "babel-preset-next": "^1.2.0",
    "copyfiles": "^2.1.1",
    "del-cli": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "nodemon": "^1.19.1",
    "renamer": "^1.1.2",
    "sinon": "^7.4.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3",
    "watch": "^1.0.2"
  },
  "ava": {
    "files": [
      "src/test/**/*"
    ],
    "require": [
      "@babel/preset-env",
      "@babel/polyfill",
      "@babel/register",
      "ts-node/register"
    ],
    "babel": "inherit",
    "extensions": [
      "js",
      "ts"
    ]
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "dotenv": "^8.1.0",
    "fs-extra": "^8.1.0",
    "merge-deep": "^3.0.2",
    "node-ini": "^1.0.0"
  }
}
