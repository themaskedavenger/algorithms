{
  "version": "0.1.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "yarn run build:lint && yarn run build:transpile",
    "build:lint": "tslint src/**/*",
    "build:transpile": "yarn run build:deletebuild && tsc",
    "dist": "yarn run dist:del && yarn run build:transpile && yarn run dist:rollup && yarn run dist:terser && yarn run dist:obfuscate && mkdir dist && cp obfuscate/start.js dist",
    "dist:del": "del 'build' && del 'rollup' && del 'terser' && del obfuscate && del 'dist'",
    "dist:obfuscate": "javascript-obfuscator terser/start.js --output obfuscate/start.js --target node",
    "dist:rollup": "rollup --config",
    "dist:terser": "mkdir terser && terser rollup/start.js -o terser/start.js -m -c --comments '/$^/'",
    "build:deletebuild": "del 'build'",
    "sandbox": "ts-node sandbox.ts",
    "start:dev": "ts-node src/start.ts",
    "start:devbuild": "yarn run build:transpile && ts-node src/start.ts",
    "start:dist": "NODE_ENV='production' node dist/start.js",
    "start:prod": "NODE_ENV='production' node build/start.js",
    "test": "ava-ts --verbose",
    "test:file": "ava-ts --verbose src/test/test.ts",
    "test:web": "yarn build && ts-node src/webtest/webtest.ts",
    "watch:dev": "chokidar 'src/**' -c 'yarn run start:dev'",
    "watch:devbuild": "chokidar 'src/**' -c 'yarn run start:devbuild'"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@enmesh/apollo": "^0.1.120",
    "@enmesh/core": "^0.1.48",
    "@enmesh/pages": "^0.1.62",
    "@enmesh/testutils": "^0.1.31",
    "@enmesh/webserver": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "ava": "^3.2.0",
    "ava-ts": "^0.25.2",
    "babel-preset-next": "^1.2.0",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "copyfiles": "^2.2.0",
    "del-cli": "^3.0.0",
    "javascript-obfuscator": "^0.24.6",
    "puppeteer": "^2.1.1",
    "renamer": "^1.1.4",
    "rollup": "^1.31.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "sinon": "^8.1.1",
    "terser": "^4.6.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.5",
    "watch": "^1.0.2"
  },
  "ava": {
    "files": [
      "src/test/**/*"
    ],
    "require": [
      "@babel/preset-env",
      "@babel/polyfill",
      "@babel/register",
      "ts-node/register"
    ],
    "babel": "inherit",
    "extensions": [
      "js",
      "ts"
    ]
  },
  "dependencies": {
    "deep-equal": "^2.0.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "google-closure-compiler": "^20200204.0.0",
    "merge-deep": "^3.0.2",
    "node-ini": "^1.0.0"
  }
}
