{
  "name": "nodesiteinit",
  "version": "0.1.0",
  "description": "",
  "main": "index.mjs",
  "scripts": {
    "build": "npm run lint && npm run test && npm run transpile",
    "copyts": "copyfiles -u 1 src/**/* ts",
    "createts": "npm run deletets && npm run copyts && npm run renamets",
    "deletebuild": "del 'build'",
    "deletets": "del 'ts'",
    "lint": "tslint src/**/*",
    "renamets": "renamer --force --find '.mts' --replace '.ts' './ts/**'",
    "sandbox": "ts-node sandbox.ts",
    "startdev": "npm run transpile && ts-node src/startdev.ts",
    "start": "NODE_ENV='production' node build/start.js",
    "test": "ava-ts --verbose",
    "testbuild": "ava --verbose build/test/**/*",
    "transpile": "npm run createts && npm run deletebuild && tsc && npm run deletets",
    "watch": "npx nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themaskedavenger/nodesiteinit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/themaskedavenger/nodesiteinit/issues"
  },
  "homepage": "https://github.com/themaskedavenger/nodesiteinit#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "ava": "^1.4.1",
    "ava-ts": "^0.25.2",
    "babel-preset-next": "^1.2.0",
    "copyfiles": "^2.1.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.11",
    "renamer": "^1.1.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.4",
    "watch": "^1.0.2"
  },
  "ava": {
    "files": [
      "src/test/**/*"
    ],
    "require": [
      "@babel/preset-env",
      "@babel/polyfill",
      "@babel/register",
      "ts-node/register"
    ],
    "babel": "inherit",
    "extensions": [
      "js",
      "ts"
    ]
  },
  "dependencies": {
    "dotenv": "^8.0.0"
  }
}
